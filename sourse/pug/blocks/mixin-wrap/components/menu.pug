//- menu
mixin mnu()
	-
		var nav= {
			"Организации":"#",
			"Вакансии":"#",
			"Профессионалы":"#",
			}
	+b.UL.navMenu
		each linkPatch, linkName in nav
			+e.LI.item: +e.A.link(href=linkPatch )= linkName
//- /menu

