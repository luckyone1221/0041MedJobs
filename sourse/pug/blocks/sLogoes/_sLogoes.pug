mixin sLogoes(data)
	// start sLogoes
	+b.SECTION.sLogoes.section#sLogoes&attributes(attributes)
		.container
			+b.section-title--lined.text-center
				h2
					| На нашем сайте
					|
					br.d-md-none
					| представлены вакансии
					br
					| частных и государственных
					|
					br.d-xl-none
					| медицинских организаций
			+e.row.row
				-for(var i = 1; i < 25; i++)
					+sLogoesItem(i)
	// end sLogoes

mixin sLogoesItem(imgNum)
	.col-4.col-md-3.col-xl-2
		+e.LABEL.item
			input(type="checkbox" class="invisible" name=`sLogoesChb${imgNum}`)
			.logo-img
				img(loading="lazy" src=`img/@2x/sLogoes-${imgNum}.png` alt="")