include ../blocks/sCards/_sCards.pug
include ../blocks/sWhat/_sWhat.pug
include ../blocks/sSearch/_sSearch.pug
include ../blocks/sDescr/_sDescr.pug

block variables
	- let  mainPage= 'new-header'
block title
	- let  title= '02-search'
	title= title
block pagecontent
	+mob-mnu5({
		logged: false,
	})
	+header()
		+top-nav5({
			logged: false,
		})
	+sSearch()
		+inpStrip('Организации', 'Введите название организации')
		+breadcrumb('Главная', 'Организации')
	+sDescr()
	+footerAlt()